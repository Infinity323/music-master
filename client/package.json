{
  "name": "MusicMaster",
  "version": "0.1.0",
  "private": true,
  "main": "./src/electron.js",
  "homepage": "./",
  "dependencies": {
    "@chakra-ui/react": "^2.5.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "canvasjs": "^1.8.3",
    "chart.js": "^4.2.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^2.1.2",
    "chartjs-plugin-zoom": "^2.0.1",
    "date-fns": "^2.29.3",
    "fs": "^0.0.1-security",
    "igniteui-react-core": "^18.1.2",
    "igniteui-react-gauges": "^18.1.2",
    "mathjs": "latest",
    "matt-diamond-recorderjs": "https://github.com/mattdiamond/Recorderjs",
    "mdbreact": "^5.2.0",
    "pitchfinder": "^2.3.2",
    "react": "^18.2.0",
    "react-bootstrap": "latest",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.0",
    "react-slider": "^2.0.4",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.5.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron-start": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-pack": "electron-builder build --publish=never"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "music.master.app",
    "productName": "MusicMaster",
    "asar": false,
    "extends": null,
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "./src/electron.js",
      "./src/music_master_backend"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "electron": "latest",
    "electron-builder": "latest",
    "wait-on": "latest"
  }
}
